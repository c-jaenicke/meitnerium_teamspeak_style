/* TabBar */
QTabWidget#permTabWidget::pane {
     	border: 1px solid #ffffff;
}
QTabBar {
	background-color: transparent;
}
QTabBar::tab {
	background: #424b55;
	border: 1px solid #000000;
	color: #000;
	min-width: 8ex;
	padding: 3px;
}
QTabBar::tab:hover {
	background: #95917d;
}
QTabBar::tab:selected {
	border-color: #424b55;
