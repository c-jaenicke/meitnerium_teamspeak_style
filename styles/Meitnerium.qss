QMainWindow, QWidget, QDialog, TextBrowser, QAbstractItemView, QTextEdit {
	background: #21262b;
	color: #ffffff;
}
QWidget#MainWindowServerTabsWidget, QWidget#MainWindowChatWidget {
	background-color: #21262b;
}
