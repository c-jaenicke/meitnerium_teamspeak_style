/* Tree View */
QTreeView {
	alternate-background-color: #21262b;
    background-color: #21262b;
	border: 1px solid #21262b;
    outline: none;
    qproperty-animated: 1;
}
TreeView {
	background: #21262b;
    border: 1px solid #21262b;
	color: #3160f2;
}
QTreeView::item {
	height: 18px;
	border: 0px;
    color: #ffffff;
}
QTreeView::item:hover {
	background: #95917d;
	color: #000000;
}
QTreeView::item:selected:active {
	background: #424b55;
	color: #ffffff;
}
QTreeView::item:selected:!active {
	background: #424b55;
	color: #ffffff;
}
QTreeView::branch:hover {
	background: #95917d;
}
/* QTreeView::branch:selected:!active -> wird vom :active ï¿½bernommen */
QTreeView::branch:selected {
	background: #424b55;
}
QTreeView::branch:has-siblings:!adjoins-item {
	border-image: url(styles:Meitnerium/sibling_branch.png) 0;
}
QTreeView::branch:has-siblings:adjoins-item {
	border-image: url(styles:Meitnerium/sibling_branch.png) 0;
}
QTreeView::branch:!has-children:!has-siblings:adjoins-item {
	border-image: url(styles:Meitnerium/sibling_branch.png) 0;
}
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
	border-image: none;
	image: url(styles:Meitnerium/open_arrow.png);
}
QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
	border-image: none;
	image: url(styles:Meitnerium/close_arrow.png);
}
QTreeView#eventsTreeView::indicator:checked, QTreeView#permissionsTreeView::indicator:checked, QTreeWidget#appsTreeWidget::indicator:checked, QListWidget#pluginsListWidget::indicator:checked, QListView#ServerGroupsListView::indicator:checked, QTreeWidget#clientsTreeWidget::indicator:checked, QGroupBox#advancedGroupBox::indicator:checked {
	background-color: #424b55;
	border: 1px solid #000000;
	border-radius: 3px;
	image: url(styles:Meitnerium/menu_check.png);
}
QTreeView#eventsTreeView::indicator:checked:hover, QTreeView#permissionsTreeView::indicator:checked:hover, QTreeWidget#appsTreeWidget::indicator:checked:hover, QListWidget#pluginsListWidget::indicator:checked:hover, QListView#ServerGroupsListView::indicator:checked:hover, QTreeWidget#clientsTreeWidget::indicator:checked:hover, QGroupBox#advancedGroupBox::indicator:checked:hover {
	background: #424b55;
}
QTreeView#eventsTreeView::indicator:unchecked, QTreeView#permissionsTreeView::indicator:unchecked, QTreeWidget#appsTreeWidget::indicator:unchecked, QListWidget#pluginsListWidget::indicator:unchecked, QListView#ServerGroupsListView::indicator:unchecked, QTreeWidget#clientsTreeWidget::indicator:unchecked, QGroupBox#advancedGroupBox::indicator:unchecked {
	background-color: #424b55;
	border: 1px solid #000000;
	border-radius: 3px;
}
QTreeView#eventsTreeView::indicator:unchecked:hover,QTreeView#permissionsTreeView::indicator:unchecked:hover, QTreeWidget#appsTreeWidget::indicator:unchecked:hover, QListWidget#pluginsListWidget::indicator:unchecked:hover, QListView#ServerGroupsListView::indicator:unchecked:hover, QTreeWidget#clientsTreeWidget::indicator:unchecked:hover, QGroupBox#advancedGroupBox::indicator:unchecked:hover {
	background: #424b55;
}
QTreeWidget#clientsTreeWidget {
	background: transparent;
}
/* ScrollBar vertical */
QScrollBar:vertical {
	border: 1px solid #000000;
	background-color: #21262b;
	width: 15px;
	margin: 17px 0 17px 0;
}
QScrollBar::handle:vertical {
	border: none;
	background: #424b55;
	min-height: 20px;
}
QScrollBar::add-line:vertical  {
	border: 1px solid #000000;
	background: #424b55;
	height: 15px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
	border: 1px solid #000000;
	background: #424b55;
	height: 15px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical {
	background: url("styles:Meitnerium/up_arrow.png");
	background-repeat: none;
	left: 3px;
	top: 5px;
}
QScrollBar::down-arrow:vertical {
	background: url("styles:Meitnerium/down_arrow.png");
	background-repeat: none;
	left: 3px;
	top: 5px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}
/* ScrollBar horizontal */
QScrollBar:horizontal {
	border: 1px solid #000000;
	background-color: #21262b;
	height: 15px;
	margin: 0px 17px 0 17px;
}
QScrollBar::handle:horizontal {
	border: none;
	background: #424b55;
	min-width: 20px;
}
QScrollBar::add-line:horizontal  {
	border: 1px solid #000000;
	background: #424b55;
	width: 15px;
	subcontrol-position: right;
	subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
	border: 1px solid #000000;
	background: #424b55;
	width: 15px;
	subcontrol-position: left;
	subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal {
	background: url("styles:Meitnerium/left_arrow.png");
	background-repeat: none;
	left: 5px;
	top: 3px;
}
QScrollBar::right-arrow:horizontal {
	background: url("styles:Meitnerium/right_arrow.png");
	background-repeat: none;
	left: 5px;
	top: 3px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}
