QTableView {
	selection-background-color: #424b55;
	selection-color: #ffffff;
	qproperty-alternatingRowColors: 1;
	border: 1px solid #ffffff;
	gridline-color: #ffffff;
}
QTableView::item {
	border: 1px solid transparent;
    color: #ffffff;
}
QTableView::item:hover {
	background: #95917d;
	border: 1px solid transparent;
	color: #ffffff;
}
QTableView::item:selected:active {
	background: #424b55;
	color: #000000;
	border: 1px solid transparent;
}
QTableView::item:selected:!active {
	background: #424b55;
	border: 1px solid transparent;
}
/*QSlider*/
QSlider {
	background: #424b55;
	padding-top: 6px;
	padding-bottom: 6px;
}
QSlider::groove:horizontal {
	background: #424b55;
	border: 1px solid transparent;
	border-radius: 3px;
	padding-right: 5px;
	padding-left: 5px;
	height: 7px;
}
QSlider::sub-page:horizontal { /*left side*/
	background: #424b55;
	border-bottom: 1px solid #000000;
	border-left: 1px solid #000000;
	border-top: 1px solid #000000;
	border-radius: 3px;
}
QSlider::add-page:horizontal { /*right side*/
	background: #424b55;
	border-bottom: 1px solid #000000;
	border-right: 1px solid #000000;
	border-top: 1px solid #000000;
	border-radius: 3px;
}
QSlider::handle:horizontal {
	background: #95917d;
	border-bottom: 1px solid #000000;
	border-right: 1px solid #000000;
	border-left: 1px solid #000000;
	border-top: 1px solid #000000;
	border-radius: 3px;
	margin: -4px 0;
	width: 7px;
}
/* Statusbar */
QStatusBar {
	background-color: #424b55;
}
/* Excluding background for specific elements */
QLabel, QCheckBox, QRadioButton, QGroupBox, QSlider, QFrame#bookmarksTopFrame, QWidget#advancedWidget, QWidget#statusChangeWidget, QWidget#connectToWidget, QWidget#switchToWidget, QWidget#disableWidget, QWidget#activateWidget, QWidget#recordingWidget, ImprovedTabBarCloseButton, QWidget#avatarWidget, QWidget#ToolbarStretcher, QStackedWidget {
	background: none;
    color: #ffffff;
}
QWidget#ConnectionInfoDialog, FriendFoeManager, ServerLogView, ClientLogView, FileBrowser, BookmarksManager, QWidget#UrlCatcherViewer, QWidget#ServerConnectionInfoDialog, LogDisplay, QDialog#BanDialog, QWidget#addPropertiesWidget, EmoticonsDisplay, QWidget#OfflineMessagesDialog, QWidget#sound3Dwidget, UserVolumeWidget, QWidget#sound3DTestWidget, QWidget#CustomizeToolbar {
	background: #21262b;
    color: #ffffff;
}
QWidget#ServerQueryView {
	background: #21262b;
    color: #ffffff;
}
QWidget#HeadlineLabel {
    background: #424b55;
}
QTextEdit#pluginDescTextEdit, QTextEdit#descriptionTextEdit, QTextEdit#vserverWelcomeTextEdit, QTextEdit#changeLogTextEdit {
	border: 1px solid #000000;
	border-radius: 3px;
	background: transparent;
    color: #ffffff;
}
MainWindowServerTabsWidget, MainWindowChatWidget {
	background: #21262b;
}
QLineEdit:disabled, QComboBox:disabled, QSpinBox:disabled {
	border: 1px solid #000000;
	background: #21262b;
	color: #95917d;
}
QCheckBox:disabled, QRadioButton:disabled, QTextEdit:disabled {
	background-color: transparent;
	color: #95917d;
}
QCheckBox::indicator:checked {
	height: 12px;
	width: 12px;
	background-color: #95917d;
	border: 1px solid #000000;
	border-radius: 3px;
	image: url(styles:Meitnerium/menu_check1.png);
}
QCheckBox::indicator:unchecked {
	height: 12px;
	width: 12px;
	background-color: #95917d;
	border: 1px solid #000000;
	border-radius: 3px;
}
QCheckBox::indicator:checked:hover {
	background: #95917d;
}
QCheckBox::indicator:unchecked:hover {
	background: #95917d;
}
/*RadioButton*/
QRadioButton::indicator:checked {
	image: url(styles:Meitnerium/radioButton1.png);
	background: #95917d;
	border: 1px solid #000000;
	border-radius: 7px;
	height: 12px;
	width: 12px;
}
QRadioButton::indicator::unchecked {
	background: #95917d;
	border: 1px solid #000000;
	border-radius: 7px;
	height: 12px;
	width: 12px;
}
QRadioButton::indicator:unchecked:hover {
	background: #424b55;
}
QRadioButton::indicator:checked:hover {
	background: #424b55;
}
QTextEdit#pluginDescTextEdit:disabled, QTextEdit#descriptionTextEdit:disabled, QTextEdit#vserverWelcomeTextEdit:disabled, QTextEdit#changeLogTextEdit:disabled {
	border: 1px solid #b1aeae;
}
QSizeGrip {
	background-color: transparent;
	image: url("styles:Meitnerium/sizegrip.png");
	width: 16px;
	height: 16px;
}
/* Transparent TeamSpeak logo in top-right corner of Infoframe text window*/
InfoFrame {
	border: 0px;
	background-repeat: none;
	background-position: top right;
	background-attachment: fixed;
    color: #ffffff;
}
QTextEdit#ChatTab {
	border: 0px;
	background-repeat: none;
	background-position: bottom right;
	background-attachment: fixed;
    color: #ffffff;
}
/* Wizard */
QWizard QWidget {
	background: #ffffff;
}
QWizard QLabel {
	background: transparent;
}
/* Options dialog title and description */
QLabel#OptionsHeadlineTitleLabel {
	background: #none;
	color: #ffffff;
	font-weight: bold;
}
QLabel#OptionsHeadlineDescriptionLabel {
	background: none;
	color: #ffffff;
}
/* Capture Hotkey dialog */
QLabel#RecorderLabel {
	border: 2px solid #000000;
	background: #21262b;
    color: #ffffff;
}
